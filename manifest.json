{
  "manifest_version": 3,
  "name": "Configurable New Tab Redirect",
  "version": "1.1",
  "description": "Redirects new tabs to a configurable URL",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "redirect.html"
  },
  "options_page": "options.html"
}
